{
  "name": "jest-mock-external-components",
  "version": "1.0.7",
  "description": "Mock external React components with jest",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/asvetliakov/jest-mock-external-components"
  },
  "author": "Alexey Svetliakov <devpieron@gmail.com>",
  "license": "MIT",
  "keywords": [
    "jest",
    "jest-snapshot",
    "testing",
    "react-testing",
    "react",
    "react-test-renderer",
    "test-renderer",
    "snapshots",
    "jest-mock",
    "react-mock"
  ],
  "scripts": {
    "clean": "del-cli build",
    "ts": "tsc -p ./",
    "babel": "babel src --source-maps --out-dir build --ignore=\"**/*.spec.*,**/__tests__/**\" --extensions=\".ts\"",
    "build": "run-p ts babel",
    "prepare": "run-s clean build"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.49",
    "@babel/core": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "@babel/preset-react": "^7.0.0-beta.49",
    "@babel/preset-stage-3": "^7.0.0-beta.49",
    "@babel/preset-typescript": "^7.0.0-beta.49",
    "@types/babel-core": "^6.25.3",
    "@types/babel-traverse": "^6.25.3",
    "@types/babel-types": "^7.0.2",
    "@types/jest": "^22.2.3",
    "@types/node": "^10.1.3",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.0.1",
    "babel-plugin-emotion": "^9.1.2",
    "del-cli": "^1.1.0",
    "jest": "^23.0.1",
    "npm-run-all": "^4.1.3",
    "typescript": "^2.9.1-insiders.20180525",
    "typescript-snapshots-plugin": "^1.2.0"
  },
  "peerDependencies": {
    "@babel/core": "^7.0.0-beta || ^7.0.0-rc || ^7.0",
    "jest": ">= 21.0"
  }
}
